{
  "name": "wommytilliams",
  "version": "1.0.0",
  "description": "jekyll 4 wommytilliams.com",
  "main": "index.js",
  "scripts": {
    "pug": "pug assets/_pug --out build/ -P -s",
    "scss": "node-sass assets/_scss/ -o build/css",
    "serve": "browser-sync start -s 'build' -f 'build' --host $IP --port $PORT",
    "dev": "parallelshell 'npm run pug -- --watch' 'npm run scss' 'npm run scss -- --watch' 'npm run serve'"
  },
  "keywords": [],
  "author": "wommy",
  "license": "Apache-2.0",
  "devDependencies": {
    "browser-sync": "^2.18.8",
    "node-sass": "^3.13.1",
    "parallelshell": "^2.0.0",
    "pug": "^2.0.0-beta9",
    "pug-cli": "^1.0.0-alpha6"
  }
}
